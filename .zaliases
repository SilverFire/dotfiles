#!/usr/bin/env zsh

clone() {
    echo git clone git@github.com:$*;
    git clone git@github.com:$*;
}

pclone() {
    echo git clone ssh://git.solex.me:222/git/$*;
    git clone ssh://git.solex.me:222/git/$*;
}

md() {
    mkdir -p $1
    cd $1
}

rehash() {
    source ~/.zshrc
    builtin rehash
}
